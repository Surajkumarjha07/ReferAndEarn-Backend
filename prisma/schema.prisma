generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model refers{
  id Int @id @default(autoincrement())
  referee_email String
  referee_name String
  referrer_email String
  referrer_name String
  voucher_code String
}
